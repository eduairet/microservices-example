{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "ascii": {
        "ClusterId": "ascii",
        "Match": {
          "Path": "/ascii/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "api/ascii/{**catch-all}"
          }
        ]
      },
      "search": {
        "ClusterId": "search",
        "Match": {
          "Path": "/search/{**catch-all}",
          "Methods": [
            "GET"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "api/v1/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "ascii": {
        "Destinations": {
          "asciiApi": {
            "Address": "http://localhost:5100/"
          }
        }
      },
      "search": {
        "Destinations": {
          "asciiApi": {
            "Address": "http://localhost:5102/"
          }
        }
      }
    }
  }
}
